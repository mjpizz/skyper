{

  "name": "skyper",
  "version": "0.0.2",
  "description": "autoinitiate Skype calls and chats using Skype URIs",

  "keywords": [
    "skype",
    "tools"
  ],

  "repository": {
      "type": "git",
      "url": "https://github.com/mjpizz/skyper.git"
  },

  "main": "lib/index.js",

  "bin": {
    "skyper": "./bin/skyper"
  },

  "dependencies": {

    "open": "0.x",
    "applescript": "0.x",
    "optimist": "0.x",

    "request": "2.x",
    "rimraf": "2.x",
    "tar": "0.x",
    "zlib": "1.x"

  },

  "virtualenv": {
    "version": "1.10.1",
    "flags": [
      "--system-site-packages"
    ],
    "dependencies": [
      "Skype4Py==1.0.35"
    ]
  },

  "scripts": {
    "postinstall": "bin/postinstall"
  },

  "license": "MIT",

  "author": {
    "name": "Matt Pizzimenti",
    "url": "http://mjpizz.com"
  }

}
